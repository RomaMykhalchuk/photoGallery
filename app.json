{
  "expo": {
    "name": "PhotoGallery",
    "slug": "PhotoGallery",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.home.name"
    },
    "android": "com.home.name",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "The gallery of free-usable beautiful pictures",
    "githubUrl": "https://github.com/RomaMykhalchuk/photoGallery"
  }
}
